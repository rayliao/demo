{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-submodule-imports": false,
    "jsx-wrap-multiline": false,
    "semicolon": [
      false,
      "always"
    ],
    "quotemark": [
      false
    ],
    "interface-name": [
      false
    ],
    "member-access": [
      false
    ],
    "jsx-no-multiline-js": [
      false
    ],
    "jsx-no-lambda": [
      false
    ],
    "max-classes-per-file": [
      false,
      2
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": true,
    "trailing-comma": [
      false
    ],
    "arrow-parens": false,
    "jsx-alignment": false,
    "jsdoc-format": false,
    "no-implicit-dependencies": false,
    "no-empty-interface": false,
    "member-ordering": false
  }
}